function Point(x, y) {
   this.x = x;
   this.y = y;
}

function Rectangle(topLeft, botRight) {
   this.topLeft = topLeft;
   this.botRight = botRight;
   this.area = function () {
      return (this.botRight.x - this.topLeft.x) *
         (this.topLeft.y - this.botRight.y);
   };
}

var p1 = new Point(0, 5);
var p2 = new Point(5, 1);

var rect1 = new Rectangle(p1, p2);

print (rect1.area());
print ("\n");

var rect2 = {};
rect2.topLeft = new Point(0, 1);
rect2.botRight = new Point(2, 0);
rect2.area = rect1.area;

print (rect2.area());
print ("\n");

